[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=kneeraazon
Group=www-data
WorkingDirectory=/home/kneeraazon/pyapps/FoodandBeverage
ExecStart=/home/kneeraazon/pyapps/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/run/gunicorn.sock \
          FoodandBeverage.wsgi:application

[Install]
WantedBy=multi-user.target
